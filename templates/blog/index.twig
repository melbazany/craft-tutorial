{% extends "_layout" %}

{% set posts = craft.entries()
.section('blog')
.with([
    [
    'featureImage', { withTransforms:
        [{
            mode: 'fit',
            width: 1200
        },{
            mode: 'thumbnail'
        }]
        }
    ]
])
.all() %}

{% block content %}
    <h1>Blog posts</h1>

    {% include "blog/_feed" with {
        posts: posts
    } only %}

{% endblock %}
